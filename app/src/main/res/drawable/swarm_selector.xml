<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <layer-list>
            <item android:drawable="@drawable/swarm_off" android:width="35dp" android:height="15dp"></item>
        </layer-list>
    </item>
    <item android:state_checked="true">
        <layer-list>
            <item android:drawable="@drawable/swarm_on" android:width="35dp" android:height="15dp"></item>
        </layer-list>
    </item>
</selector>